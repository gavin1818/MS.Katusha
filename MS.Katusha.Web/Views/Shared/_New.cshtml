@using MS.Katusha.Enumerations
@{ var profiles = ViewBag.KatushaNewProfiles as IEnumerable<ProfileModel>; }
@if (profiles != null) {
    <h3>New Users</h3>
    <div id="newgallery" data-toggle="modal-gallery" data-target="#modal-gallery">
        @foreach (var profile in profiles) {
            @Html.DisplayProfilePhoto(profile, PhotoType.Icon, "newgallery", true)
        }
    </div>
    @Html.ActionLink("More...", "New", "Profiles", null, new { style = "font-weight:bold" });
}
