                    </p>
                </div>
                <div class="float-right">
                    <section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>
                    @if (ViewBag.KatushaProfile == null) {
                        <nav>
                            <ul id="menu">
                                <li>@Html.ActionLink("Home", "Index", "Home")</li>
                                <li>@Html.ActionLink("Men", "Men", "Profiles")</li>
                                <li>@Html.ActionLink("Girls", "Girls", "Profiles")</li>
                            </ul>
                        </nav>
                    }
