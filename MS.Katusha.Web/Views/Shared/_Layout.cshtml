        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                <div id="k_main" class="k_clearfix">
                    <div id="k_main_sidebar_left">@Html.Partial("_LeftPartial")</div>
